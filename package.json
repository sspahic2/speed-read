{
  "name": "speed-reader-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/run-next.js dev",
    "build": "dotenv -e .env -- npx prisma generate && node scripts/run-next.js build",
    "start": "next start",
    "migrate-diff": "dotenv -e .env -- npx prisma migrate diff --from-config-datasource \"$DATABASE_URL\" --to-schema prisma/schema.prisma --script --output migrate-diff/migration.sql",
    "migrate-deploy": "dotenv -e .env -- npx prisma migrate deploy",
    "generate": "dotenv -e .env -- npx prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vercel/blob": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.555.0",
    "next": "16.0.7",
    "next-auth": "^4.24.13",
    "prisma": "^7.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "shadcn": "^3.5.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
